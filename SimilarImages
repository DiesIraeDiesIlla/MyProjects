from PIL import Image
from PIL import ImageChops
import math

print("Enter a name of file â„–1")
img1=Image.open(input())
width1, height1 = img1.size
print(width1, height1)

print("Enter a name of file â„–2")
img2=Image.open(input())
width2, height2 = img2.size
print(width2, height2)

diff = ImageChops.difference(img1, img2)
h = diff.histogram()
sq = (value * ((idx % 256)**2) for idx, value in enumerate(h))
sum_of_squares = sum(sq)
rms = math.sqrt(sum_of_squares/float(img1.size[0] * img1.size[1]))
 

while width1==width2 & height1==height2:
    if rms==0:
        print("Images are the same rms=",rms)
        print(img1)
    elif rms>90 & rms<100:
        print("Images are similar rms=",rms)
        print(img1,img2)

if width1!=width2 & height1!=height2:
    print("Images are not the same rms=",rms)
 
 
